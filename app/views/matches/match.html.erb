<% if current_user %>
    <% if @in_match %>
        Yer in a match bro
        <%= render :partial => 'battlefield', :locals => {:match_id => @match}%>
    <% else %>
        Not in match, checking proof of concept
        //Add matchmaking here
        <h1>Choose deck:</h1>
        <%= select("deck", "user", @decks, {include_blank: false}) %>
        <button id="startMatchMake" class="button">Matchmake</button>
    <% end %>
<% end %>